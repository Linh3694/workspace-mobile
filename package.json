{
  "name": "wis-mobile",
  "version": "1.2.22",
  "scripts": {
    "android": "npm run copy-firebase && expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "prebuild": "expo prebuild && npm run copy-firebase",
    "copy-firebase": "bash ./scripts/copy-firebase-config.sh",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "deploy": "./scripts/deploy.sh",
    "deploy:android": "./scripts/deploy.sh android",
    "deploy:ios": "./scripts/deploy.sh ios",
    "deploy:submit": "./scripts/deploy.sh all true",
    "build:android": "eas build --platform android --profile production",
    "build:ios": "eas build --platform ios --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest",
    "release:android": "./scripts/build-android.sh",
    "release:android:build-only": "./scripts/build-android.sh --no-submit",
    "release:ios": "./scripts/build-ios.sh",
    "release:ios:build-only": "./scripts/build-ios.sh --no-submit",
    "clean-all": "rm -rf node_modules && npm install && expo start --clear",
    "check-apple-signin": "node scripts/check-apple-signin.js"
  },
  "dependencies": {
    "@callstack/liquid-glass": "^0.6.0",
    "@expo/vector-icons": "^15.0.3",
